# File will be loaded as environment variables
# It must contain strings like:
# name=value

GITLEAKS_LOG_LEVEL=warn

# If set to false, one needs to resolve all clones prior.
# All JSCPD errors are fixed at 21/05/2025 by @kagw95.
# Now it reports only 1 yaml and 2 pairs of java classes (in import section).
# I consider it False-Positive, so JSCPD is turned off.
VALIDATE_JSCPD=false

VALIDATE_KUBERNETES_KUBECONFORM=false
VALIDATE_MARKDOWN_PRETTIER=false
VALIDATE_YAML_PRETTIER=false

# We don't have GO modules
VALIDATE_GO=false
